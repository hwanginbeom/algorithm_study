    "a, b = input().split()\n",
    "a = int(a)\n",
    "b = int(b)\n",
    "\n",
    "def gcd(a, b):\n",
    "    if a % b == 0:\n",
    "        return b\n",
    "    else:\n",
    "        return gcd(b, a % b)\n",
    "\n",
    "gcd = gcd(a,b)\n",
    "print(gcd)\n",
    "lcm = (a//gcd)*(b//gcd)*gcd\n",
    "print(lcm)"
   ]
