    "while True :\n",
    "    data = list(map(int, input().split()))\n",
    "    data.sort()\n",
    "\n",
    "    if data[0] == 0 and data[1] == 0 and data[2] == 0:\n",
    "        break\n",
    "\n",
    "    if ((data[2]**2) == (data[0]**2 + data[1]**2)) :\n",
    "        print('right')\n",
    "    else :\n",
    "        print('wrong')"
   ]
